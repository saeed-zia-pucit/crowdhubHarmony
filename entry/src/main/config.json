{
  "app": {
    "bundleName": "com.example.crowdhubharmony",
    "vendor": "example",
    "version": {
      "code": 1000001,
      "name": "1.0.1"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.example.crowdhubharmony",
    "name": ".MyApplication",
    "mainAbility": "com.example.crowdhubharmony.MainAbility",
    "allowClassMap": true,
    "deviceType": [
      "wearable"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "name": "com.example.crowdhubharmony.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:entry_MainAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "backgroundModes": [],
        "skills": [
          {
            "actions": [
              "com.huawei.push.action.MESSAGING_EVENT"
            ]
          }
        ],
        "name": "com.example.crowdhubharmony.services.MessagingService",
        "icon": "$media:icon",
        "description": "MessagingService",
        "type": "service",
        "visible": false,
        "permissions": []
      },
      {
        "orientation": "unspecified",
        "name": "com.example.crowdhubharmony.SplashActivity",
        "icon": "$media:icon",
        "description": "$string:splashactivity_description",
        "label": "$string:entry_SplashActivity",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.example.crowdhubharmony.TestAbility",
        "icon": "$media:icon",
        "description": "$string:testability_description",
        "label": "$string:entry_TestAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.example.crowdhubharmony.DeviceIdAbility",
        "icon": "$media:icon",
        "description": "$string:deviceidability_description",
        "label": "$string:entry_DeviceIdAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.example.crowdhubharmony.PingNotificationScreen",
        "icon": "$media:icon",
        "description": "$string:pingnotificationscreen_description",
        "label": "$string:entry_PingNotificationScreen",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.example.crowdhubharmony.SplashAbility",
        "icon": "$media:icon",
        "description": "$string:splashability_description",
        "label": "$string:entry_SplashAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.example.crowdhubharmony.LocationAbility",
        "icon": "$media:icon",
        "description": "$string:locationability_description",
        "label": "$string:entry_LocationAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.example.crowdhubharmony.FusedLocationAbility",
        "icon": "$media:icon",
        "description": "$string:fusedlocationability_description",
        "label": "$string:entry_FusedLocationAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "backgroundModes": [
          "dataTransfer",
          "location"
        ],
        "name": "com.example.crowdhubharmony.LocationService",
        "icon": "$media:icon",
        "description": "$string:locationservice_description",
        "type": "service"
      },
      {
        "orientation": "unspecified",
        "name": "com.example.crowdhubharmony.MapAbility",
        "icon": "$media:icon",
        "description": "$string:mapability_description",
        "label": "$string:entry_MapAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.example.crowdhubharmony.MapDirectionAbility",
        "icon": "$media:icon",
        "description": "$string:mapdirectionability_description",
        "label": "$string:entry_MapDirectionAbility",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.LOCATION",
        "reason": "string:reason_description",
        "usedScene": {
          "ability": [
            "com.example.crowdhubharmony.LocationAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.LOCATION_IN_BACKGROUND",
        "reason": "string:reason_description_backgroun",
        "usedScene": {
          "ability": [
            "com.example.crowdhubharmony.LocationAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.GET_NETWORK_INFO"
      },
      {
        "name": "ohos.permission.SET_NETWORK_INFO"
      },
      {
        "name": "ohos.permission.INTERNET"
      }
    ]
  }
}